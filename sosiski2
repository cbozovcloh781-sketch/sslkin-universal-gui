        if not gp and input.UserInputType == Enum.UserInputType.Keyboard then
            conn:Disconnect()
            NoClipConfig.ForceToggleKey = input.KeyCode
            forceNoClipKeyBtn.Text = "Force NoClip Hotkey: [" .. tostring(input.KeyCode.Name) .. "]"
        end
    end)
end)


