-- Кнопка для принудительного поиска предметов
local forceSearchItemsBtn = Instance.new("TextButton", innerContainer)
forceSearchItemsBtn.Size = UDim2.new(1, -10, 0, 28)
forceSearchItemsBtn.Text = "FORCE SEARCH YBA ITEMS"
forceSearchItemsBtn.Font = Enum.Font.GothamBold
forceSearchItemsBtn.TextSize = 12
forceSearchItemsBtn.TextColor3 = Color3.new(1,1,1)
forceSearchItemsBtn.BackgroundColor3 = Color3.fromRGB(100,255,100)
forceSearchItemsBtn.AutoButtonColor = false
Instance.new("UICorner", forceSearchItemsBtn).CornerRadius = UDim.new(0,6)

forceSearchItemsBtn.MouseButton1Click:Connect(function()
    print("YBA: Принудительный поиск предметов YBA...")
    local success, objects = pcall(getAllObjects)
    if success and objects then
        print("YBA: Найдено предметов YBA:", #objects)
        for i = 1, math.min(10, #objects) do
            local item = objects[i]
            print("YBA: Предмет", i, ":", item.Name, "на расстоянии", math.floor(item.Distance), "м")
        end
    else
        print("YBA: Ошибка при поиске предметов YBA")
    end
end)

-- Кнопка для тестирования ESP на ближайшем объекте
local testESPBtn = Instance.new("TextButton", innerContainer)
testESPBtn.Size = UDim2.new(1, -10, 0, 28)
testESPBtn.Text = "TEST ESP ON NEAREST YBA ITEM"
testESPBtn.Font = Enum.Font.GothamBold
testESPBtn.TextSize = 12
testESPBtn.TextColor3 = Color3.new(1,1,1)
testESPBtn.BackgroundColor3 = Color3.fromRGB(100,100,255)
testESPBtn.AutoButtonColor = false
Instance.new("UICorner", testESPBtn).CornerRadius = UDim.new(0,6)

testESPBtn.MouseButton1Click:Connect(function()
    print("YBA: Тестируем ESP на ближайшем предмете YBA...")
    local success, objects = pcall(getAllObjects)
    if success and objects and #objects > 0 then
        local nearestObject = objects[1]
        print("YBA: Ближайший предмет YBA:", nearestObject.Name, "на расстоянии", math.floor(nearestObject.Distance), "м")
        
        -- Создаем ESP для этого предмета
        local esp = createItemESP(nearestObject)
        if esp then
            print("YBA: ESP успешно создан для:", nearestObject.Name)
        else
            print("YBA: Ошибка при создании ESP для:", nearestObject.Name)
        end
    else
        print("YBA: Предметы YBA не найдены или произошла ошибка")
    end
end)

-- Конец файла
