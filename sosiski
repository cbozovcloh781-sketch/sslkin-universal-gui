versionText.Text = "V55"
versionText.Font = Enum.Font.Gotham
versionText.TextSize = 12
versionText.TextColor3 = Color3.new(1, 1, 1)
versionText.BackgroundTransparency = 1
versionText.TextXAlignment = Enum.TextXAlignment.Center

-- Навигационные кнопки
local navButtons = {
    {name = "Player", icon = "👤"},
    {name = "Executor", icon = "⚡"},
    {name = "Script Hub", icon = "📜"},
    {name = "Games Hub", icon = "🎮"},
    {name = "Settings", icon = "⚙️"}
}

local selectedTab = "Games Hub"

for i, buttonData in ipairs(navButtons) do
    local navButton = Instance.new("TextButton", leftPanel)
    navButton.Name = buttonData.name .. "Button"
    navButton.Size = UDim2.new(1, -20, 0, 40)
    navButton.Position = UDim2.new(0, 10, 0, 100 + (i-1) * 50)
    navButton.Text = buttonData.icon .. " " .. buttonData.name
    navButton.Font = Enum.Font.Gotham
    navButton.TextSize = 14
    navButton.TextColor3 = Color3.new(1, 1, 1)
    navButton.BackgroundColor3 = buttonData.name == selectedTab and Color3.fromRGB(40, 40, 45) or Color3.fromRGB(30, 30, 35)
    navButton.BorderSizePixel = 0
    navButton.AutoButtonColor = false
    
    local buttonCorner = Instance.new("UICorner", navButton)
    buttonCorner.CornerRadius = UDim.new(0, 6)
    
    -- Подсветка выбранной вкладки
    if buttonData.name == selectedTab then
        local highlight = Instance.new("Frame", navButton)
        highlight.Name = "Highlight"
        highlight.Size = UDim2.new(0, 3, 1, 0)
        highlight.Position = UDim2.new(0, 0, 0, 0)
        highlight.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        highlight.BorderSizePixel = 0
        
        local highlightCorner = Instance.new("UICorner", highlight)
        highlightCorner.CornerRadius = UDim.new(0, 2)
    end
    
    navButton.MouseButton1Click:Connect(function()
        -- Обновляем выбранную вкладку
        for _, btn in pairs(leftPanel:GetChildren()) do
            if btn:IsA("TextButton") then
                btn.BackgroundColor3 = Color3.fromRGB(30, 30, 35)
                local highlight = btn:FindFirstChild("Highlight")
                if highlight then
                    highlight:Destroy()
                end
            end
        end
        
        navButton.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
        selectedTab = buttonData.name
        
        -- Добавляем подсветку
        local highlight = Instance.new("Frame", navButton)
        highlight.Name = "Highlight"
        highlight.Size = UDim2.new(0, 3, 1, 0)
        highlight.Position = UDim2.new(0, 0, 0, 0)
        highlight.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        highlight.BorderSizePixel = 0
        
        local highlightCorner = Instance.new("UICorner", highlight)
        highlightCorner.CornerRadius = UDim.new(0, 2)
        
        -- Показываем соответствующий контент
        showContent(buttonData.name)
    end)
end

-- Правая панель контента
local rightPanel = Instance.new("Frame", mainFrame)
rightPanel.Name = "RightPanel"
rightPanel.Size = UDim2.new(1, -120, 1, 0)
rightPanel.Position = UDim2.new(0, 120, 0, 0)
rightPanel.BackgroundColor3 = Color3.fromRGB(35, 35, 40)
rightPanel.BorderSizePixel = 0

local rightCorner = Instance.new("UICorner", rightPanel)
rightCorner.CornerRadius = UDim.new(0, 8)

-- Кнопка закрытия
local closeButton = Instance.new("TextButton", rightPanel)
closeButton.Name = "CloseButton"
closeButton.Size = UDim2.new(0, 30, 0, 30)
closeButton.Position = UDim2.new(1, -35, 0, 5)
closeButton.Text = "X"
closeButton.Font = Enum.Font.GothamBold
closeButton.TextSize = 16
closeButton.TextColor3 = Color3.new(1, 1, 1)
closeButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
closeButton.BorderSizePixel = 0
closeButton.AutoButtonColor = false

local closeCorner = Instance.new("UICorner", closeButton)
closeCorner.CornerRadius = UDim.new(0, 6)

closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)

-- Заголовок контента
local contentTitle = Instance.new("TextLabel", rightPanel)
contentTitle.Name = "ContentTitle"
contentTitle.Size = UDim2.new(1, -20, 0, 40)
contentTitle.Position = UDim2.new(0, 10, 0, 10)
contentTitle.Text = "GLOBAL FUNCTIONS"
contentTitle.Font = Enum.Font.GothamBold
contentTitle.TextSize = 18
contentTitle.TextColor3 = Color3.new(1, 1, 1)
contentTitle.BackgroundTransparency = 1
contentTitle.TextXAlignment = Enum.TextXAlignment.Center

-- Контейнер для кнопок функций
local functionsContainer = Instance.new("Frame", rightPanel)
functionsContainer.Name = "FunctionsContainer"
functionsContainer.Size = UDim2.new(1, -20, 1, -80)
functionsContainer.Position = UDim2.new(0, 10, 0, 60)
functionsContainer.BackgroundTransparency = 1

local functionsLayout = Instance.new("UIListLayout", functionsContainer)
functionsLayout.Padding = UDim.new(0, 10)
functionsLayout.SortOrder = Enum.SortOrder.LayoutOrder

-- Функции для создания кнопок
local function createFunctionButton(text, callback)
    local button = Instance.new("TextButton", functionsContainer)
    button.Size = UDim2.new(1, 0, 0, 50)
    button.Text = text
    button.Font = Enum.Font.GothamBold
    button.TextSize = 16
    button.TextColor3 = Color3.new(1, 1, 1)
    button.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
    button.BorderSizePixel = 0
    button.AutoButtonColor = false
    
    local buttonCorner = Instance.new("UICorner", button)
    buttonCorner.CornerRadius = UDim.new(0, 8)
    
    -- Эффект при наведении
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(50, 50, 55)
        }):Play()
    end)
    
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(40, 40, 45)
        }):Play()
    end)
    
    if callback then
        button.MouseButton1Click:Connect(callback)
    end
    
    return button
end

-- Создаем кнопки функций
local speedHackButton = createFunctionButton("SPEED HACK", function()
    SpeedHackConfig.Enabled = not SpeedHackConfig.Enabled
    if SpeedHackConfig.Enabled then
        startSpeedHack()
    else
        stopSpeedHack()
    end
end)

local btoolsButton = createFunctionButton("BTOOLS", function()
    -- Функция для BTOOLS
    print("BTOOLS activated")
end)

local antiCheatButton = createFunctionButton("ROBLOX ANTI-CHEAT BYPASS", function()
    -- Функция для обхода анти-чита
    print("Anti-cheat bypass activated")
end)

local feFlingButton = createFunctionButton("FE FLING", function()
    -- Функция для FE FLING
    print("FE FLING activated")
end)

-- Статус Speed Hack внизу
local statusFrame = Instance.new("Frame", rightPanel)
statusFrame.Name = "StatusFrame"
statusFrame.Size = UDim2.new(1, -20, 0, 30)
statusFrame.Position = UDim2.new(0, 10, 1, -35)
statusFrame.BackgroundTransparency = 1

local speedHackStatus = Instance.new("TextLabel", statusFrame)
speedHackStatus.Name = "SpeedHackStatus"
speedHackStatus.Size = UDim2.new(0, 200, 1, 0)
speedHackStatus.Position = UDim2.new(0, 0, 0, 0)
speedHackStatus.Text = "Speed Hack: 0"
speedHackStatus.Font = Enum.Font.Gotham
speedHackStatus.TextSize = 14
speedHackStatus.TextColor3 = Color3.new(1, 1, 1)
speedHackStatus.BackgroundTransparency = 1
speedHackStatus.TextXAlignment = Enum.TextXAlignment.Left

-- Слайдер для Speed Hack
local speedSlider = Instance.new("Frame", statusFrame)
speedSlider.Name = "SpeedSlider"
speedSlider.Size = UDim2.new(0, 20, 0, 20)
speedSlider.Position = UDim2.new(0, 210, 0, 5)
speedSlider.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
speedSlider.BorderSizePixel = 0

local sliderCorner = Instance.new("UICorner", speedSlider)
sliderCorner.CornerRadius = UDim.new(0.5, 0)

-- Функция для показа контента
function showContent(tabName)
    -- Очищаем контейнер
    for _, child in pairs(functionsContainer:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end
    
    if tabName == "Games Hub" then
        -- Показываем глобальные функции
        contentTitle.Text = "GLOBAL FUNCTIONS"
        
        createFunctionButton("SPEED HACK", function()
            SpeedHackConfig.Enabled = not SpeedHackConfig.Enabled
            if SpeedHackConfig.Enabled then
                startSpeedHack()
            else
                stopSpeedHack()
            end
        end)
        
        createFunctionButton("BTOOLS", function()
            print("BTOOLS activated")
        end)
        
        createFunctionButton("ROBLOX ANTI-CHEAT BYPASS", function()
            print("Anti-cheat bypass activated")
        end)
        
        createFunctionButton("FE FLING", function()
            print("FE FLING activated")
        end)
        
    elseif tabName == "Player" then
        contentTitle.Text = "PLAYER FUNCTIONS"
        
        createFunctionButton("FLY", function()
            FlyConfig.Enabled = not FlyConfig.Enabled
            if FlyConfig.Enabled then
                startFly()
            else
                stopFly()
            end
        end)
        
        createFunctionButton("NOCLIP", function()
            if isNoClipping then
                stopNoClip()
            else
                startNoClip()
            end
        end)
        
        createFunctionButton("INFINITE JUMP", function()
            InfiniteJumpConfig.Enabled = not InfiniteJumpConfig.Enabled
        end)
        
    elseif tabName == "Executor" then
        contentTitle.Text = "EXECUTOR FUNCTIONS"
        
        createFunctionButton("EXECUTE SCRIPT", function()
            print("Script execution")
        end)
        
        createFunctionButton("CLEAR OUTPUT", function()
            print("Output cleared")
        end)
        
    elseif tabName == "Script Hub" then
        contentTitle.Text = "SCRIPT HUB"
        
        createFunctionButton("LOAD SCRIPTS", function()
            print("Loading scripts")
        end)
        
        createFunctionButton("SCRIPT MANAGER", function()
            print("Script manager")
        end)
        
    elseif tabName == "Settings" then
        contentTitle.Text = "SETTINGS"
        
        createFunctionButton("CONFIGURATION", function()
            print("Configuration")
        end)
        
        createFunctionButton("THEME SETTINGS", function()
            print("Theme settings")
        end)
    end
end

-- Показываем контент по умолчанию
showContent("Games Hub")

-- Обновление статуса Speed Hack
RunService.Heartbeat:Connect(function()
    if SpeedHackConfig.Enabled then
        speedHackStatus.Text = "Speed Hack: " .. SpeedHackConfig.Speed
        speedHackStatus.TextColor3 = Color3.fromRGB(0, 255, 0)
    else
        speedHackStatus.Text = "Speed Hack: 0"
        speedHackStatus.TextColor3 = Color3.new(1, 1, 1)
    end
end)

-- Переключение меню по Insert
UserInputService.InputBegan:Connect(function(input, gp)
    if not gp and input.KeyCode == Enum.KeyCode.Insert then
        mainFrame.Visible = not mainFrame.Visible
    end
end)
